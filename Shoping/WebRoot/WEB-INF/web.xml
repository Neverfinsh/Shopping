  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindUser</servlet-name>
    <url-pattern>/FindUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ModifyGoods</servlet-name>
    <url-pattern>/modifyGoods</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GiveMoney</servlet-name>
    <url-pattern>/GiveMoney</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteDingdan</servlet-name>
    <url-pattern>/deleteDingdan</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
