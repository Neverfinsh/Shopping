  <display-name>Shoping</display-name>
  <servlet>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>com.pro.servlet.UserLogin</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
